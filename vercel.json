{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/blog",
      "destination": "/blog/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/partners",
      "destination": "/partenaires",
      "permanent": true
    },
    {
      "source": "/resources",
      "destination": "/ressources",
      "permanent": true
    },
    {
      "source": "/testimonials",
      "destination": "/temoignages",
      "permanent": true
    },
    {
      "source": "/back-office",
      "destination": "/services/back-office.html",
      "permanent": true
    },
    {
      "source": "/externalisation-service-client",
      "destination": "/services/externalisation-service-client.html",
      "permanent": true
    },
    {
      "source": "/gestion-reclamations",
      "destination": "/services/gestion-reclamations.html",
      "permanent": true
    },
    {
      "source": "/helpdesk",
      "destination": "/services/helpdesk.html",
      "permanent": true
    },
    {
      "source": "/permanence",
      "destination": "/services/permanence.html",
      "permanent": true
    },
    {
      "source": "/prospection",
      "destination": "/services/prospection.html",
      "permanent": true
    },
    {
      "source": "/relance-commerciale",
      "destination": "/services/relance-commerciale.html",
      "permanent": true
    },
    {
      "source": "/secretariat-telephonique",
      "destination": "/services/secretariat-telephonique.html",
      "permanent": true
    },
    {
      "source": "/service-client",
      "destination": "/services/service-client.html",
      "permanent": true
    },
    {
      "source": "/standard-externalise",
      "destination": "/services/standard-externalise.html",
      "permanent": true
    },
    {
      "source": "/telesecretariat",
      "destination": "/services/telesecretariat.html",
      "permanent": true
    },
    {
      "source": "/partenaires",
      "destination": "/partners/index.html",
      "permanent": true
    },
    {
      "source": "/ressources",
      "destination": "/resources/index.html",
      "permanent": true
    },
    {
      "source": "/temoignages",
      "destination": "/temoignages.html",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
