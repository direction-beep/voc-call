{
  "outputDirectory": ".",
  "buildCommand": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer-when-downgrade"
        }
      ]
    },
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=3600, stale-while-revalidate=86400"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)\\.(woff2|woff|ttf|otf)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/blog",
      "destination": "/blog/index.html",
      "statusCode": 301
    },
    {
      "source": "/blog/",
      "destination": "/blog/index.html",
      "statusCode": 301
    },
    {
      "source": "/carte-france.html",
      "destination": "/carte-france",
      "statusCode": 301
    },
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "statusCode": 301
    },
    {
      "source": "/test-css",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/test-css.html",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/test-css/",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/index.html",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/gestion-reclamations",
      "destination": "/services/gestion-reclamations.html",
      "statusCode": 301
    },
    {
      "source": "/helpdesk",
      "destination": "/services/helpdesk.html",
      "statusCode": 301
    },
    {
      "source": "/permanence",
      "destination": "/services/permanence.html",
      "statusCode": 301
    },
    {
      "source": "/prospection",
      "destination": "/services/prospection.html",
      "statusCode": 301
    },
    {
      "source": "/relance-commerciale",
      "destination": "/services/relance-commerciale.html",
      "statusCode": 301
    },
    {
      "source": "/service-client",
      "destination": "/services/service-client.html",
      "statusCode": 301
    },
    {
      "source": "/standard-externalise",
      "destination": "/services/standard-externalise.html",
      "statusCode": 301
    },
    {
      "source": "/telesecretariat",
      "destination": "/services/telesecretariat.html",
      "statusCode": 301
    },
    {
      "source": "/back-office",
      "destination": "/services/back-office.html",
      "statusCode": 301
    },
    {
      "source": "/externalisation-service-client",
      "destination": "/services/externalisation-service-client.html",
      "statusCode": 301
    },
    {
      "source": "/secretariat-telephonique",
      "destination": "/services/secretariat-telephonique.html",
      "statusCode": 301
    },
    {
      "source": "/guides/calculateur-roi-service-client.csv",
      "destination": "/resources/guides/calculateur-roi-service-client.csv",
      "statusCode": 301
    },
    {
      "source": "/guides/pdfs/Checklist-Choisir-Call-Center-France.pdf",
      "destination": "/resources/guides/checklist-choisir-call-center-france.html",
      "statusCode": 301
    },
    {
      "source": "/guides/pdfs/Guide-Externalisation-Service-Client-2025.pdf",
      "destination": "/resources/guides/guide-externalisation-service-client-2025.html",
      "statusCode": 301
    },
    {
      "source": "/guides/pdfs/Guide-Homeshoring-Recrutement.pdf",
      "destination": "/resources/guides/guide-homeshoring-recrutement.html",
      "statusCode": 301
    },
    {
      "source": "/guides/pdfs/Guide-KPI-Service-Client.pdf",
      "destination": "/resources/guides/guide-kpi-service-client.html",
      "statusCode": 301
    },
    {
      "source": "/guides/pdfs/Guide-RGPD-Call-Center.pdf",
      "destination": "/resources/guides/guide-rgpd-call-center.html",
      "statusCode": 301
    },
    {
      "source": "/guides/pdfs/Template-Contrat-Externalisation.pdf",
      "destination": "/resources/guides/template-contrat-externalisation.html",
      "statusCode": 301
    },
    {
      "source": "/:slug.html",
      "destination": "/:slug",
      "statusCode": 301
    }
  ],
  "rewrites": [
    {
      "source": "/devis-:slug",
      "destination": "/api/devis-redirect?slug=:slug"
    },
    {
      "source": "/demenageur-:slug",
      "destination": "/api/demenageur-redirect?slug=:slug"
    },
    {
      "source": "/css/styles.min.css",
      "destination": "/css/styles.css"
    },
    {
      "source": "/js/:file.min.js",
      "destination": "/js/:file.js"
    }
  ]
}
